{
    "Id" : "google-chrome",
    "TryFile": "/opt/google/chrome/chrome",
    "Apply": {
        "InitUsage": 10,
        "After": [ { "scope": "snapshot", "id:": "DDE" } ],
        "When": [ {"id":"idle"}, {"scope":"dde", "id":"dde-launcher"}]
    },
    "Capture": {
        "ExpireLimit": 0,
        "Method": [
            {
                "Type": "uiapp",
                "WMClass": "deepin-terminal"
            },
            {
                "Type": "filelist",
                "FileList": ["$HOME/.config/google-chrome"],
                "Blacklist": [
                    "$HOME/.config/google-chrome/Crash Reports",
                    "$HOME/.config/google-chrome/Webstore Downloads"
                ]
            }
        ]
    }
}
