{
    "Id" : "DDE",
    "Description": "stuff about DDE",
    "TryFile": "/usr/bin/startdde",
    "Apply": {
        "After": [ {"scope":"snapshot", "id":"BASIC"} ]
    },
    "Capture": {
        "ExpireLimit": 0,
        "UseMincore": true,
        "WhenAfter": ["dde:ready"],
        "Method": [
            {
                "Type": "mincores",
                "Mincores": ["/", "$HOME"],
                "Blacklist": ["/var/lib/lastore", "$HOME/.config/pulse"]
            }
        ]
    }
}
