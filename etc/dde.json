{
    "Id" : "DE",
    "Description": "stuff about DDE",
    "TryFile": "/usr/bin/startdde",
    "Apply": {
        "Before": [ "systemd:user.slice"],
        "After": [ "snapshot:BASIC" ]
    },
    "Capture": {
        "After": [ "dde:idle" ],
        "Before": [ "snapshot:DE"],
        "Method": [
            {
                "Type": "mincores",
                "Mincores": ["/", "$HOME"],
                "Blacklist": ["/var/lib/lastore", "$HOME/.config/pulse"]
            }
        ]
    }
}
